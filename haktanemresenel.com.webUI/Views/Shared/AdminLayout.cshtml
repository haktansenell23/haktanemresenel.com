<!DOCTYPE html>
<html lang="tr" ng-app="app">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"]</title>
    <link href="~/tailwindcss/tailwind.min.css" rel="stylesheet" />
    <link href="~/tailwindcss/components.min.css" rel="stylesheet" />
    <link href="~/tailwindcss/utilities.min.css" rel="stylesheet" />
    <link rel="stylesheet" href="~/haktanemresenel.com.webUI.styles.css" asp-append-version="true" />
    <link href="~/font-awesome/css/all.min.css" rel="stylesheet" />
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link href="~/scss/site.min.css" rel="stylesheet" asp-append-version="true" />

</head>
<body>


    <audio controls="controls" style="display:none;" id="generalTheme">
        <source src="~/media/starwarsmaintheme.mp3" type="audio/mpeg" />
    </audio>


    <div class="h-100 container-fluid rounded  overflow-auto" ng-controller="AdminCtrl" ng-show="isLoading">
               
                    @RenderBody()
               
    </div>
@* 
    <div class="container" ng-controller="MainCtrl" ng-show="isLoading">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div> *@


    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/vanilla.js" asp-append-version="true"></script>

    <script src="~/angular.js/angular.min.js"></script>
    <script src="~/js/modules.js" asp-append-version="true"></script>
    <script src="~/js/maincontroller.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
    <script>

        var homeUrl = window.location.host;

        console.log("homeUrl", homeUrl);
    </script>
</body>
</html>
